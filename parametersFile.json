{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus"
    },
    "domain": {
      "value": "[toLower(substring(uniqueString(resourceGroup().id), 0, 8))]"
    },
    "clusterVnetName": {
      "value": "aro-vnet"
    },
    "clusterVnetCidr": {
      "value": "10.100.0.0/15"
    },
    "workerSubnetCidr": {
      "value": "10.100.70.0/23"
    },
    "masterSubnetCidr": {
      "value": "10.100.76.0/24"
    },
    "masterVmSize": {
      "value": "Standard_D8s_v3"
    },
    "workerVmSize": {
      "value": "Standard_D2s_v3"
    },
    "workerVmDiskSize": {
      "value": 128
    },
    "workerCount": {
      "value": 3
    },
    "podCidr": {
      "value": "10.128.0.0/14"
    },
    "serviceCidr": {
      "value": "172.30.0.0/16"
    },
    "clusterName": {
      "value": null
    },
    "tags": {
      "value": {
        "env": "Dev",
        "dept": "Ops"
      }
    },
    "apiServerVisibility": {
      "value": "Public"
    },
    "ingressVisibility": {
      "value": "Public"
    },
    "aadClientId": {
      "value": null
    },
    "aadObjectId": {
      "value": null
    },
    "aadClientSecret": {
      "value": null
    },
    "rpObjectId": {
      "value": null
    },
    "spguid": {
      "value": "[newGuid()]"
    },
    "rpguid": {
      "value": "[newGuid()]"
    }
  }
}
